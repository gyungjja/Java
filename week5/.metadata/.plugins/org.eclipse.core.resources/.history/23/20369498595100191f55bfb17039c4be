package week5;

public class Person implements IComparable {
	private String name;
	private float height;
	//생성자
	Person(){
		
	}
	Person(String n, float h){
		this.name = n;
		this.height = h;
		
	}
	
	@Override
	public String toString() {
		return "Person [name=" + name + ", height=" + height + "]";
	}
	//인터페이스의 추상 메서드 구현
	int compareTo(Person other) {
		if( > other)
			return 1;
		else if( == other)
			return 0;
		else
			return -1;
	}
	

}

