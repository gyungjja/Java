package week6;

public class Person {
	String name;
	int age;
	int num;
	
	Person(){
		this("none",0);
		
	}
	Person(String name, int age){
		this.name = name;
		this.age = age;
	}
	@Override
	public String toString() {
		return "Person [name=" + name + ", age=" + age + ", num=" + num + "]";
	}
	

}
