package week6;

public class Person {
	//필드 : name, age, num(학번 or 사번)
	private String name;	//이름
	private int age;	//나이
	private int num;	//학번 or 사번
	
	//생성자:name,age 필드 사용
	Person(){
		this("anonymous",0);
	}
	Person(String name, int age){
		this.name = name;
		this.age = age;
	}
	@Override
	public String toString() {
		return "Person [name=" + name + ", age=" + age +  "]";
	}
	
	//메소드:toString()
	

}

