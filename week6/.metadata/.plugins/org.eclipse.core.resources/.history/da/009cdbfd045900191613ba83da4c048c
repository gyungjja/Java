package week6;

import java.util.Random;
public class Student extends Person implements IJob,IPin{
	//필드: enter(입학년도),pay
	int enter;
	double pay;
	
	//생성자:enter,일한시간 변수사용 학번 구현
	Student(){
		this(0);
	}
	Student(int enter){
		this.enter = enter;
	}
	//인터페이스 추상메서드
	public double getPay(double hour) {
		if(a instanceof b)
			return hour*7000;
		else
			return hour*8000;
	}
	
	public int getPin() {
		return (int)(1+10*Math.random());
	}
	@Override
	public String toString() {
		return "Student [enter=" + enter + ", pay=" + pay + "]";
	}
	
	//메서드 : toString
	

}
